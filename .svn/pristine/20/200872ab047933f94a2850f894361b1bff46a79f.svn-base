html {
    width: 100%;
    height: 100%;
}

@media only screen and (min-width: 100px) and (max-width: 375px) {
    body {
        font-size: 12px;
    }
}

@media only screen and (min-width: 376px) {
    body {
        font-size: 12px;
    }
}

body {
    color: #808080;
    margin: 0px;
    padding: 0px;
    -webkit-text-size-adjust: none;
    font-family: "-apple-system", "Helvetica Neue", Roboto, "Segoe UI", sans-serif;
}

.guesspane {
    overflow: scroll;
}

.white {
    color: #FFFFFF;
}

.yellow {
    color: #ff5b06;
}

.gray {
    color: #808080;
}

.blue {
    color: #3a66b3;
}

.back {
    color: #666666;
}

.textindent {
    text-indent: 5px;
}

.block {
    display: block;
}

.inlineblock {
    display: inline-block;
}

.floatleft {
    float: left;
    overflow: hidden;
}

.floatright {
    float: right;
    overflow: hidden;
}

.textCut {
    word-spacing: initial;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.scrollpane {
    overflow: scroll;
}


/*
 * 赔率显示比例
 */

.threeDiv {
    width: 33.33%;
}

    .threeDiv > span:first-child {
        /*width: 60%;
        display: inline-block;
        line-height: 2em;
        vertical-align: middle;
        text-indent: 0;*/
        width: 65%;
        white-space: nowrap;
        text-overflow: ellipsis;
        overflow: hidden;
        float: left;
    }

.twoDiv {
    width: 50%;
}

    .twoDiv > span:first-child {
        /*width: 70%;
        display: inline-block;
        line-height: 2em;
        vertical-align: middle;
        text-indent: 0;*/
        width: 75%;
        white-space: nowrap;
        text-overflow: ellipsis;
        overflow: hidden;
        float: left;
    }

.fourDiv {
    width: 25%;
}

.fiveDiv {
    width: 20%;
}

.shadow {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.2);
}

.shadowload {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.selectBet,
.selectBet .back,
.selectBet .yellow {
    background-color: #3a66b3;
    color: #ffffff !important;
}

.icon {
    background-image: url('../image/icon.png');
    background-repeat: no-repeat;
}

.sportbox {
