.selectsport {
    height: 35px;
    width: 60px;
    max-width: 100%;
    padding-top: 2.5px;
    margin: auto;
}

.sportpane {
    position: absolute;
    width: 64px;
    left: -4px;
    top: 73px;
    margin-left: 1.2em;
    margin-left: calc((25% - 70px)/2);
    margin-left: -webkit-calc((25% - 70px)/2);
    margin-left: -moz-calc((25% - 70px)/2);
    background-color: #ffffff;
    border: solid 1px #cccccc;
    border-radius: 0.5em;
    z-index: 1;
}

    .sportpane a:first-child {
        border-bottom: solid 1px #CCCCCC;
    }

    .sportpane a {
    }

    .sportpane:before {
        position: absolute;
        top: -5px;
        content: "";
        width: 7px;
        height: 7px;
        left: 29px;
        transform: rotate(45deg);
        -webkit-transform: rotate(45deg);
        -moz-transform: rotate(45deg);
        -ms-transform: rotate(45deg);
        border-top: solid 1px #cccccc;
        border-left: solid 1px #cccccc;
        background-color: #FFFFFF;
    }

.verticalcenter {
    display: flex;
    align-items: center;
    /*垂直居中*/
    justify-content: center;
    /*水平居中*/
}

.textScale {
    transform: scale(0.8);
    -webkit-transform: scale(0.8);
    -moz-transform: scale(0.8);
    -ms-transform: scale(0.8);
}

.wgicon {
    background-image: url(../image/icon.png);
    background-repeat: no-repeat;
}

.icon_balance {
    width: 28px;
    height: 28px;
    margin: 0.2em 0.5em 0 1em;
    background-position: -121px -2px;
    vertical-align: middle;
    background-size: 1600%;
    margin-top: -webkit-calc((3em - 33px)/2);
}

.icon_balance_bet {
    width: 28px;
    height: 28px;
    background-position: -170px -13px;
    vertical-align: middle;
    transform: scale(0.65);
    margin-top: 2px;
}

.title span.yellow {
    font-size: 12px;
}

.icon_statement {
    background-position: -51px -51px;
    margin-right: 10px;
    margin-top: 0.3em;
    margin-top: calc((3em - 21px)/2);
    margin-top: -webkit-calc((3em - 21px)/2);
    margin-top: -moz-calc((3em - 21px)/2);
    width: 21px;
    height: 21px;
    background-size: 1600%;
}


.icon_ranking {
    background-position: -51px -78px;
    margin-top: 0.3em;
    margin-top: calc((3em - 21px)/2);
    margin-top: -webkit-calc((3em - 21px)/2);
    margin-top: -moz-calc((3em - 21px)/2);
    width: 21px;
    height: 21px;
    background-size: 1600%;
    margin-right: 14px;
}

.icon_up {
    width: 20px;
    height: 20px;
    margin-right: 4px;
    margin-top: 0.45em;
    margin-top: calc((3.5em - 24px)/2);
    margin-top: -webkit-calc((3.5em - 24px)/2);
    margin-top: -moz-calc((3.5em - 24px)/2);
    background-position: -107px 1px;
    background-size: 1600%;
}

.icon_down {
    width: 20px;
    height: 20px;
    margin-right: 4px;
    margin-top: 0.45em;
    margin-top: calc((3.5em - 24px)/2);
    margin-top: -webkit-calc((3.5em - 24px)/2);
    margin-top: -moz-calc((3.5em - 24px)/2);
    background-position: -125px 1px;
    background-size: 1600%;
}

.icon_close {
    width: 1.5em;
    height: 1.5em;
    margin: 0.5em 0.5em 0 0;
    background-position: -270px -15px;
    transform: scale(0.65);
    display: inline-block;
    float: right;
}

.icon_closeitem {
    margin: 0.5em 1em 0 0;
    background-position: -296px -18px;
    float: right;
    width: 15px;
    height: 15px !important;
}
