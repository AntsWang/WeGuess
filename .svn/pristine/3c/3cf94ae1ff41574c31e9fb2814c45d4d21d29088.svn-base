.no-data-tips {
    text-align: center;
    margin-top: 48%;
    display: inline-block;
    width: 100%;
}

.inrightpane {
    margin-left: -22%;
}

::-webkit-scrollbar {
    width: 0px;
    height: 0px;
}


@font-face {
    font-family: "Ionicons";
    src: url("/fonts/ionicons.eot?v=2.0.1");
    src: url("/fonts/ionicons.eot?v=2.0.1#iefix") format("embedded-opentype"), url("../fonts/ionicons.ttf?v=2.0.1") format("truetype"), url("../fonts/ionicons.woff?v=2.0.1") format("woff"), url("../fonts/ionicons.woff") format("woff"), url("../fonts/ionicons.svg?v=2.0.1#Ionicons") format("svg");
    font-weight: normal;
    font-style: normal;
}

.rule_cont {
    display: inline-block;
    width: 100%;
    height: 100%;
}

.rule_iframe {
    width: 100%;
    height: 100%;
    border: none;
}

.debug {
    position: fixed;
    top: 10px;
    height: 85%;
    overflow: scroll;
    width: 95%;
    display: none;
    background-color: rgba(200,200,200,0.4);
    z-index: 999999999;
    color: #00f;
}

    .debug.active {
        display: block !important;
    }

    .debug .close {
        float: right;
        width: 20px;
        height: 20px;
        display: inline-block;
        background-color: gray;
        top: 10px;
        position: fixed;
        right: 20px;
        font-size: 20px;
        line-height: 20px;
        text-align: center;
        color: red;
    }

.debug_c p {
    margin-top: 2px;
    margin-bottom: 2px;
    text-indent: 5px;
}

#cmdline {
    display: inline-block;
    position: fixed;
    bottom: 1px;
    width: 95%;
}

    #cmdline #ipt {
        width: 95%;
        height: 40px;
    }

    #cmdline button {
        height: 30px;
    }

.touch_scroll {
    -webkit-overflow-scrolling: touch;
}
