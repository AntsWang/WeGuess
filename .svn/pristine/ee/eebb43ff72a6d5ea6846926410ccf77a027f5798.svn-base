.conpub .publishfooter a:last-child {
    color: #3A66B3;
}
.conpub .successpane .matchlist{
	height: 100px;
}
.conpub .successpane .matchlist .row1{
	height: 40px;
	padding: 10px 10px 0 10px;
	display: flex;
	font-size: 14px;
}
.conpub .successpane .matchlist .row1 div{
	float: left;
	height: 40px;
	line-height: 40px;
	text-align: center;
	flex: 1;
	color: #000;
}
.conpub .successpane .matchlist .row1 div.vsa{
	color: #808080;
}
.conpub .successpane .matchlist .row1 .time span{
	display: block;
	height: 20px;
	line-height: 20px;
	color:#808080;
}
.conpub .successpane .matchlist .row2{
	height: 40px;
	line-height: 40px;
	text-align: center;
	color:#FF5B06;
	font-size:14px;
}
.publishfooter a{
	font-size: 14px;
}
.messagetitle{
	font-size: 14px;
}
