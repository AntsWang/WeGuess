/*
 *加载页
 */
(function (w, fun) {
    if (w.component == null) {
        w.component = {};
    }
    w.component['loading'] = fun(w);
})(window, function (w) {
    var loadinghtml = '<div class="shadowload loading">\
						<img src="context/image/loading1.gif" ></img>\
						<div class="blue"><span class="textCut">{{message}}</span></div>\
					 </div>';
    var loading = Vue.extend({
        template: loadinghtml,
        props: ["message"]
    });
    return loading;
});